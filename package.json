{
  "name": "GenFit_Mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "node fix-expo-windows.js",
    "start": "expo start",
    "start:unix": "EXPO_NO_METRO_LAZY=1 expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "node fix-expo-windows.js && expo start --web",
    "web:clear": "node fix-expo-windows.js && expo start --web --clear",
    "web:alt": "expo start --tunnel",
    "clean": "expo start --clear"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-dom": "18.2.0",
    "react-native-web": "~0.19.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-native-async-storage/async-storage": "1.18.2",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "@expo/webpack-config": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
